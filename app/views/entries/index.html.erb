<h1>All Entries</h1>
<div>
 <% @entries.each do |entry| %>
   <div class="container">
      <div class="row">
        <%= entry_form(entry, true) %>
      </div>
  </div>
   <%= button_to "Delete", entry, method: :delete, data: { confirm: "Are you sure you want to delete this Project ?"} %>
 <% end %>
</div>